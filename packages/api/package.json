{
  "name": "api",
  "version": "1.0.0",
  "description": "api server for a translation platform",
  "main": "lib/index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "eslint",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node $npm_package_main",
    "dev": "ts-node-dev --respawn src/",
    "lint:all": "npm run lint \"src/**/*.ts\""
  },
  "engines": {
    "node": "12"
  },
  "eslintConfig": {
    "extends": "airbnb-base"
  },
  "dependencies": {
    "@apollo/federation": "0.16.2",
    "apollo-server-errors": "2.5.0",
    "apollo-server-express": "2.14.2",
    "axios": "0.24.0",
    "bcryptjs": "2.4.3",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "graphql": "15.0.0",
    "graphql-iso-date": "3.6.1",
    "graphql-middleware": "4.0.2",
    "graphql-shield": "7.3.0",
    "graphql-upload": "11.0.0",
    "jsonwebtoken": "9.0.0",
    "mongoose": "5.9.16",
    "ramda": "0.27.0",
    "slug": "3.3.0"
  },
  "devDependencies": {
    "@types/graphql": "14.5.0",
    "@types/mongoose": "5.10.0",
    "eslint": "7.1.0",
    "eslint-config-airbnb-base": "14.1.0",
    "ts-node-dev": "1.0.0",
    "typescript": "4.2.4"
  }
}
